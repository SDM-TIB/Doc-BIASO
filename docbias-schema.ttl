<https://github.com/tibonto/Doc-BIAS/> rdf:type owl:Ontology ;
                                        owl:versionIRI <https://github.com/tibonto/Doc-BIAS/0.0.1> ;
									     <http://purl.org/dc/elements/1.1/contributor> "Mayra Russo" ,
                                                                                    "Yukti Sharma" ,
                                                                                    "Maria-Esther Vidal" ; 
                                      <http://purl.org/dc/elements/1.1/creator> "NoBIAS project" ;
                                      <http://purl.org/dc/terms/publisher> "TIB – Leibniz Information Centre for Science and Technology, Hannover, Germany" ;
                                      <http://purl.org/dc/terms/license> "GPL-3.0 license" ;
                                      <http://purl.org/dc/terms/created> "2022-06-10" ;
                                      <http://purl.org/dc/terms/language> "en" ;
                                      <http://purl.org/dc/elements/1.1/title> "Doc-Bias Ontology"@en ;
                                      <http://purl.org/dc/elements/1.1/description> "DocBias, an ontology to document fake news classification pipelines".										

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/citation
<http://purl.org/dc/elements/1.1/citation> rdf:type owl:AnnotationProperty ;
                                           <http://purl.org/dc/elements/1.1/identifier> "citation"@en .


###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/identifier
<http://purl.org/dc/elements/1.1/identifier> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#comment
<http://www.w3.org/2004/02/skos/core#comment> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
<http://www.w3.org/2004/02/skos/core#definition> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/dc/terms/Creator
dct:Creator rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain dcat:Dataset ;
            rdfs:range foaf:Person ;
            rdfs:comment "defines creator of the Dataset" .


###  http://purl.org/dc/terms/language
dct:language rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain dcat:Dataset ;
             rdfs:range dct:LinguisticSystem ;
             rdfs:comment "defines the language used in Dataset" .


###  http://purl.org/dc/terms/publisher
dct:publisher rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain dcat:Dataset ;
              rdfs:range foaf:Agent ;
              rdfs:comment "defines publisher of the Dataset" .


###  http://purl.org/dc/terms/temporal
dct:temporal rdf:type owl:ObjectProperty ;
             rdfs:domain dcat:Dataset ;
             rdfs:range dct:PeriodOfTime ;
             rdfs:comment "defines the period of time from which news is collected" .


###  https://github.com/tibonto/Doc-BIAS/checks
<https://github.com/tibonto/Doc-BIAS/checks> rdf:type owl:ObjectProperty ;
                                                      rdfs:subPropertyOf owl:topObjectProperty ;
                                                      rdfs:domain <https://github.com/tibonto/Doc-BIAS/FactChecker> ;
                                                      rdfs:range nobias:NewsSource ;
                                                      rdfs:comment "defines Fact Checker checking a News" .


###  https://github.com/tibonto/Doc-BIAS/classifies
<https://github.com/tibonto/Doc-BIAS/classifies> rdf:type owl:ObjectProperty ;
                                                          rdfs:domain doam:Output ;
                                                          rdfs:range <https://github.com/tibonto/Doc-BIAS/NewsClassification> ;
                                                          rdfs:comment "defines class into which news is classified by the model" .


###  https://github.com/tibonto/Doc-BIAS/follows
<https://github.com/tibonto/Doc-BIAS/follows> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf owl:topObjectProperty ;
                                                       rdfs:domain <https://github.com/tibonto/Doc-BIAS/SocialMediaUser> ;
                                                       rdfs:range <https://github.com/tibonto/Doc-BIAS/SocialMediaUser> ;
                                                       rdfs:comment "defines SocialMediaUser following another  SocialMediaUser" .


###  https://github.com/tibonto/Doc-BIAS/hasActorType
<https://github.com/tibonto/Doc-BIAS/hasActorType> rdf:type owl:ObjectProperty ;
                                                            rdfs:domain nobias:Actor ;
                                                            rdfs:range <https://github.com/tibonto/Doc-BIAS/ActorType> ;
                                                            rdfs:comment "defines Actor type: active or passive for Actor" .


###  https://github.com/tibonto/Doc-BIAS/hasArchitechture
<https://github.com/tibonto/Doc-BIAS/hasArchitechture> rdf:type owl:ObjectProperty ;
                                                                rdfs:subPropertyOf owl:topObjectProperty ;
                                                                rdfs:domain mls:Model ;
                                                                rdfs:range <https://github.com/tibonto/Doc-BIAS/Architecture> ;
                                                                rdfs:comment "defines Architecture of the Model" .


###  https://github.com/tibonto/Doc-BIAS/hasBiasRecipient
<https://github.com/tibonto/Doc-BIAS/hasBiasRecipient> rdf:type owl:ObjectProperty ;
                                                                owl:inverseOf <https://github.com/tibonto/Doc-BIAS/hasBiasSource> ;
                                                                rdfs:domain <https://github.com/tibonto/Doc-BIAS/BiasSourceAgent> ;
                                                                rdfs:range <https://github.com/tibonto/Doc-BIAS/BiasRecipientAgent> ;
                                                                rdfs:comment "describes the entity that has a bias recipient" .


###  https://github.com/tibonto/Doc-BIAS/hasBiasSource
<https://github.com/tibonto/Doc-BIAS/hasBiasSource> rdf:type owl:ObjectProperty ;
                                                             rdfs:domain <https://github.com/tibonto/Doc-BIAS/BiasRecipientAgent> ;
                                                             rdfs:range <https://github.com/tibonto/Doc-BIAS/BiasSourceAgent> ;
                                                             rdfs:comment "describes the entity that has a bias source" .


###  https://github.com/tibonto/Doc-BIAS/hasBiasType
<https://github.com/tibonto/Doc-BIAS/hasBiasType> rdf:type owl:ObjectProperty ;
                                                           rdfs:domain <https://github.com/tibonto/Doc-BIAS/BiasEvent> ;
                                                           rdfs:range <https://github.com/tibonto/Doc-BIAS/BiasType> ;
                                                           rdfs:comment "describes the type of bias detected during the Bias Event" .


###  https://github.com/tibonto/Doc-BIAS/hasClassifier
<https://github.com/tibonto/Doc-BIAS/hasClassifier> rdf:type owl:ObjectProperty ;
                                                             rdfs:subPropertyOf owl:topObjectProperty ;
                                                             rdfs:domain mls:Model ;
                                                             rdfs:range <https://github.com/tibonto/Doc-BIAS/Classifier> ;
                                                             rdfs:comment "defines Classification algorithm used to  classify News" .


###  https://github.com/tibonto/Doc-BIAS/hasContentType
<https://github.com/tibonto/Doc-BIAS/hasContentType> rdf:type owl:ObjectProperty ;
                                                              rdfs:subPropertyOf owl:topObjectProperty ;
                                                              rdfs:domain dcat:Dataset ;
                                                              rdfs:range <https://github.com/tibonto/Doc-BIAS/NewsContentType> ;
                                                              rdfs:comment "defines the type of content is used in news articles like text, images, videos etc." .


###  https://github.com/tibonto/Doc-BIAS/hasDetectionMethod
<https://github.com/tibonto/Doc-BIAS/hasDetectionMethod> rdf:type owl:ObjectProperty ;
                                                                  rdfs:domain <https://github.com/tibonto/Doc-BIAS/BiasEvent> ;
                                                                  rdfs:range <https://github.com/tibonto/Doc-BIAS/BiasDetectionMethod> ;
                                                                  rdfs:comment "describes the detection method for detecting bias during Bias Event" .


###  https://github.com/tibonto/Doc-BIAS/hasEvalMetrics
<https://github.com/tibonto/Doc-BIAS/hasEvalMetrics> rdf:type owl:ObjectProperty ;
                                                              rdfs:subPropertyOf owl:topObjectProperty ;
                                                              rdfs:domain mls:ModelEvaluation ;
                                                              rdfs:range mls:EvaluationProcedure ;
                                                              rdfs:comment "defines the type of Metrics that is used to compare different Runs" .


###  https://github.com/tibonto/Doc-BIAS/hasHyperparameter
<https://github.com/tibonto/Doc-BIAS/hasHyperparameter> rdf:type owl:ObjectProperty ;
                                                                 rdfs:subPropertyOf owl:topObjectProperty ;
                                                                 rdfs:domain mls:Process ;
                                                                 rdfs:range mls:HyperParameter ;
                                                                 rdfs:comment "defines the weights need to be trained for classification" .


###  https://github.com/tibonto/Doc-BIAS/hasInput
<https://github.com/tibonto/Doc-BIAS/hasInput> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf owl:topObjectProperty ;
                                                        rdfs:domain mls:Model ;
                                                        rdfs:range dcat:Dataset ;
                                                        rdfs:comment "defines input to the Model in form of  Dataset" ,
                                                                     "defines input to the Model in form of Datase" .


###  https://github.com/tibonto/Doc-BIAS/hasLearningMethod
<https://github.com/tibonto/Doc-BIAS/hasLearningMethod> rdf:type owl:ObjectProperty ;
                                                                 rdfs:subPropertyOf owl:topObjectProperty ;
                                                                 rdfs:domain mls:HyperParameter ;
                                                                 rdfs:range <https://github.com/tibonto/Doc-BIAS/LearningMethod> ;
                                                                 rdfs:comment "defines the method used to train or calculate the weights or hyperparameters" .


###  https://github.com/tibonto/Doc-BIAS/hasMediaPlatform
<https://github.com/tibonto/Doc-BIAS/hasMediaPlatform> rdf:type owl:ObjectProperty ;
                                                                rdfs:subPropertyOf owl:topObjectProperty ;
                                                                rdfs:domain dcat:Dataset ;
                                                                rdfs:range <https://github.com/tibonto/Doc-BIAS/MediaPlatform> ;
                                                                rdfs:comment "defines Media Platform that is used by users who share news" .


###  https://github.com/tibonto/Doc-BIAS/hasModelComponents
<https://github.com/tibonto/Doc-BIAS/hasModelComponents> rdf:type owl:ObjectProperty ;
                                                                  rdfs:subPropertyOf owl:topObjectProperty ;
                                                                  rdfs:domain doam:Predictive_Model ;
                                                                  rdfs:range doam:Model_Components ;
                                                                  rdfs:comment "defines components of Model based on doam ontology" .


###  https://github.com/tibonto/Doc-BIAS/hasModelType
<https://github.com/tibonto/Doc-BIAS/hasModelType> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf owl:topObjectProperty ;
                                                            rdfs:domain mls:Model ;
                                                            rdfs:range doam:Predictive_Model ;
                                                            rdfs:comment "defines the type of Model based on doam ontology" .


###  https://github.com/tibonto/Doc-BIAS/hasOptimizer
<https://github.com/tibonto/Doc-BIAS/hasOptimizer> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf owl:topObjectProperty ;
                                                            rdfs:domain <https://github.com/tibonto/Doc-BIAS/Classifier> ;
                                                            rdfs:range <https://github.com/tibonto/Doc-BIAS/OptimizationTechnique> ;
                                                            rdfs:comment "defines Optimization algorithm used to optimize Classifier results" .


###  https://github.com/tibonto/Doc-BIAS/hasOutput
<https://github.com/tibonto/Doc-BIAS/hasOutput> rdf:type owl:ObjectProperty ;
                                                         rdfs:subPropertyOf owl:topObjectProperty ;
                                                         rdfs:domain mls:Model ,
                                                                     mls:Run ;
                                                         rdfs:range mls:ModelEvaluation ,
                                                                    doam:Output ;
                                                         rdfs:comment "defines output of the Model as doam:Output class" ,
                                                                      "defines the output of the Model as a result of the Run" .


###  https://github.com/tibonto/Doc-BIAS/hasRating
<https://github.com/tibonto/Doc-BIAS/hasRating> rdf:type owl:ObjectProperty ;
                                                         rdfs:domain dcat:Dataset ;
                                                         rdfs:range <https://github.com/tibonto/Doc-BIAS/RatingScale> ;
                                                         rdfs:comment "defines Rating Scale is used to rate publishers" .


###  https://github.com/tibonto/Doc-BIAS/hasRecipientParticipant
<https://github.com/tibonto/Doc-BIAS/hasRecipientParticipant> rdf:type owl:ObjectProperty ;
                                                                       rdfs:domain <https://github.com/tibonto/Doc-BIAS/BiasEvent> ;
                                                                       rdfs:range nobias:BiasRecipient ;
                                                                       rdfs:comment "describes the recipient participant participating in a Bias Event" .


###  https://github.com/tibonto/Doc-BIAS/hasSource
<https://github.com/tibonto/Doc-BIAS/hasSource> rdf:type owl:ObjectProperty ;
                                                         rdfs:subPropertyOf owl:topObjectProperty ;
                                                         rdfs:domain <https://github.com/tibonto/Doc-BIAS/News> ;
                                                         rdfs:range nobias:NewsSource ;
                                                         rdfs:comment "defines News Source  for a News" .


###  https://github.com/tibonto/Doc-BIAS/hasSourceParticipant
<https://github.com/tibonto/Doc-BIAS/hasSourceParticipant> rdf:type owl:ObjectProperty ;
                                                                    rdfs:subPropertyOf owl:topObjectProperty ;
                                                                    rdfs:domain <https://github.com/tibonto/Doc-BIAS/BiasEvent> ;
                                                                    rdfs:range <https://github.com/tibonto/Doc-BIAS/BiasSource> ;
                                                                    rdfs:comment "describes the source participant participating in a Bias Event" .


###  https://github.com/tibonto/Doc-BIAS/hasTrainingProcess
<https://github.com/tibonto/Doc-BIAS/hasTrainingProcess> rdf:type owl:ObjectProperty ;
                                                                  rdfs:subPropertyOf owl:topObjectProperty ;
                                                                  rdfs:domain mls:Model ;
                                                                  rdfs:range mls:Process ;
                                                                  rdfs:comment "defines the training process of training the model" .


###  https://github.com/tibonto/Doc-BIAS/isPartOf
<https://github.com/tibonto/Doc-BIAS/isPartOf> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf owl:topObjectProperty ;
                                                        rdfs:domain mls:Experiment ;
                                                        rdfs:range mls:Study ;
                                                        rdfs:comment "defines the Study which conducts the experiment on given Dataset" .


###  https://github.com/tibonto/Doc-BIAS/performsRecipientRole
<https://github.com/tibonto/Doc-BIAS/performsRecipientRole> rdf:type owl:ObjectProperty ;
                                                                     rdfs:domain mls:Model ,
                                                                                 nobias:Actor ;
                                                                     rdfs:range <https://github.com/tibonto/Doc-BIAS/BiasRecipientAgent> ;
                                                                     rdfs:comment "describes the entity that performs recipient role for the recipient agent" .


###  https://github.com/tibonto/Doc-BIAS/performsSourceRole
<https://github.com/tibonto/Doc-BIAS/performsSourceRole> rdf:type owl:ObjectProperty ;
                                                                  rdfs:domain mls:Model ,
                                                                              nobias:Actor ;
                                                                  rdfs:range <https://github.com/tibonto/Doc-BIAS/BiasSourceAgent> ;
                                                                  rdfs:comment "describes the entity that performs source role for the source agent" .


###  https://github.com/tibonto/Doc-BIAS/preparesData
<https://github.com/tibonto/Doc-BIAS/preparesData> rdf:type owl:ObjectProperty ;
                                                            rdfs:domain <https://github.com/tibonto/Doc-BIAS/Pre-processing> ;
                                                            rdfs:range dcat:Dataset ;
                                                            rdfs:comment "defines files that \\\\ are used in preparing the Data for training to be used by Model" .


###  https://github.com/tibonto/Doc-BIAS/providesRecipientRole
<https://github.com/tibonto/Doc-BIAS/providesRecipientRole> rdf:type owl:ObjectProperty ;
                                                                     rdfs:domain nobias:BiasRecipient ;
                                                                     rdfs:range <https://github.com/tibonto/Doc-BIAS/BiasRecipientAgent> ;
                                                                     rdfs:comment "describes the entity that provides recipient role for the recipient agent" .


###  https://github.com/tibonto/Doc-BIAS/providesSourceRole
<https://github.com/tibonto/Doc-BIAS/providesSourceRole> rdf:type owl:ObjectProperty ;
                                                                  rdfs:domain <https://github.com/tibonto/Doc-BIAS/BiasSource> ;
                                                                  rdfs:range <https://github.com/tibonto/Doc-BIAS/BiasSourceAgent> ;
                                                                  rdfs:comment "describes the entity that provides source role for the source agent" .


###  https://github.com/tibonto/Doc-BIAS/publishes
<https://github.com/tibonto/Doc-BIAS/publishes> rdf:type owl:ObjectProperty ;
                                                         rdfs:subPropertyOf owl:topObjectProperty ;
                                                         rdfs:domain nobias:NewsSource ;
                                                         rdfs:range <https://github.com/tibonto/Doc-BIAS/News> ;
                                                         rdfs:comment "defines News published by a News Source" .


###  https://github.com/tibonto/Doc-BIAS/sharedBy
<https://github.com/tibonto/Doc-BIAS/sharedBy> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf owl:topObjectProperty ;
                                                        rdfs:domain <https://github.com/tibonto/Doc-BIAS/News> ;
                                                        rdfs:range <https://github.com/tibonto/Doc-BIAS/SharingBehaviour> ;
                                                        rdfs:comment "defines News that is  being shared" .


###  https://github.com/tibonto/Doc-BIAS/shares
<https://github.com/tibonto/Doc-BIAS/shares> rdf:type owl:ObjectProperty ;
                                                      rdfs:subPropertyOf owl:topObjectProperty ;
                                                      rdfs:domain <https://github.com/tibonto/Doc-BIAS/SocialMediaUser> ;
                                                      rdfs:range <https://github.com/tibonto/Doc-BIAS/SharingBehaviour> ;
                                                      rdfs:comment "defines SocialMediaUser that shares the News" .


###  http://www.w3.org/ns/dcat#theme
dcat:theme rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain dcat:Dataset ;
           rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ;
           rdfs:comment "defines genre of the Dataset" .


###  http://www.w3.org/ns/dqv#computedOn
dqv:computedOn rdf:type owl:ObjectProperty ;
               rdfs:domain dcat:Dataset ;
               rdfs:range dqv:QualityMeasurement ;
               rdfs:comment "defines measure of metric which is computed on dataset" .


###  http://www.w3.org/ns/dqv#hasQualityAnnotation
dqv:hasQualityAnnotation rdf:type owl:ObjectProperty ;
                         rdfs:domain dcat:Dataset ;
                         rdfs:range dqv:QualityAnnotation ;
                         rdfs:comment "defines annotations related to quality that Dataset has" .


###  http://www.w3.org/ns/dqv#inCategory
dqv:inCategory rdf:type owl:ObjectProperty ;
               rdfs:domain dqv:Dimension ;
               rdfs:range dqv:Category ;
               rdfs:comment "defines Category that given Dimension belongs to" .


###  http://www.w3.org/ns/dqv#inDimension
dqv:inDimension rdf:type owl:ObjectProperty ;
                rdfs:domain dqv:Metric ;
                rdfs:range dqv:Dimension ;
                rdfs:comment "defines Dimension who has given Metric" .


###  http://www.w3.org/ns/dqv#isMeasurementOf
dqv:isMeasurementOf rdf:type owl:ObjectProperty ;
                    rdfs:domain dqv:QualityMeasurement ;
                    rdfs:range dqv:Metric ;
                    rdfs:comment "defines metric which is measured" .


###  http://www.w3.org/ns/mls#executes
mls:executes rdf:type owl:ObjectProperty ;
             rdfs:domain mls:Run ;
             rdfs:range <https://github.com/tibonto/Doc-BIAS/Classifier> ;
             rdfs:comment "defines the algorithm that is executed by the Run" .


###  http://www.w3.org/ns/mls#hasInput
mls:hasInput rdf:type owl:ObjectProperty ;
             rdfs:domain mls:Run ;
             rdfs:range dcat:Dataset ;
             rdfs:comment "defines the Dataset which is used an input for the Run" .


###  http://www.w3.org/ns/mls#realizes
mls:realizes rdf:type owl:ObjectProperty ;
             rdfs:domain mls:Run ;
             rdfs:range mls:Model ;
             rdfs:comment "defines the model that is realized by the Run" .


###  http://www.w3.org/ns/odrl/2/target
odrl:target rdf:type owl:ObjectProperty ;
            rdfs:domain dqv:QualityPolicy ;
            rdfs:range dcat:Dataset ;
            rdfs:comment "defines dataset for which a quality policy is applicable for" .


###  http://www.w3.org/ns/prov#used
prov:used rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain prov:Activity ;
          rdfs:range dcat:Dataset ;
          rdfs:comment "defines the Dataset  that is used for provenance" .


###  http://www.w3.org/ns/prov#wasGeneratedBy
prov:wasGeneratedBy rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain nobias:Actor ;
                    rdfs:range prov:Activity ;
                    rdfs:comment "defines the provenance Activity that generates the Actor" .


###  https://www.openriskmanual.org/ns/doam#has_RuleSet
doam:has_RuleSet rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain doam:Rule_Set_Model ;
                 rdfs:range doam:Rule_Set ;
                 rdfs:comment "defines the rules that Rule set model has" .


